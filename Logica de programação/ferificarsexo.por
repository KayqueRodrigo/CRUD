programa {
  funcao inicio() {
    caracter sexo

    faca{
      escreva("Informe qual o seu sexo: ")
      leia(sexo)

      se(sexo == "F"){
        escreva("seu sexo é feminino")
      }se(sexo == "M"){
        escreva("Seu sexo é masculino")
      }
    }enquanto(sexo != "F" e sexo != "M")


  }
}
